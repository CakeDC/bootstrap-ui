{
    "name": "friendsofcake/bootstrap-ui",
    "description": "Twitter Bootstrap 3 support for CakePHP 3",
    "type": "cakephp-plugin",
    "keywords": ["cakephp", "twitter", "bootstrap"],
    "homepage": "http://github.com/friendsofcake/bootstrap-ui",
    "license": "MIT",
    "authors": [
        {
            "name": "Jad Bitar",
            "homepage": "http://jadb.io",
            "role": "Author"
        },
        {
            "name": "Others",
            "homepage": "https://github.com/friendsofcake/bootstrap-ui/graphs/contributors"
        }
    ],
    "prefer-stable": true,
    "minimum-stability": "dev",
    "require": {
        "cakephp/cakephp": "4.x-dev as 4.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7",
        "cakephp/bake": "4.x-dev as 4.0.0",
        "vlucas/phpdotenv": "^3.3",
        "rector/rector": "^0.4.11",
        "cakephp/cakephp-codesniffer": "dev-master",
        "phpstan/phpstan": "^0.11.0@dev"
    },
    "support": {
        "issues": "http://github.com/friendsofcake/bootstrap-ui/issues",
        "source": "http://github.com/friendsofcake/bootstrap-ui"
    },
    "autoload": {
        "psr-4": {
            "BootstrapUI\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BootstrapUI\\Test\\": "tests",
            "TestApp\\": "tests/test_app/TestApp"
        }
    },
    "scripts": {
        "check": [
            "@test",
            "@cs-check"
        ],
        "cs-check": "phpcs -p --standard=vendor/cakephp/cakephp-codesniffer/CakePHP src/ tests/",
        "cs-fix": "phpcbf --standard=vendor/cakephp/cakephp-codesniffer/CakePHP src/ tests/",
        "test": "phpunit"
    }
}
